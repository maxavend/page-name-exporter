{
  "name": "page-name-exporter",
  "version": "1.0.0",
  "description": "Figma Page Topology Manager (Exporter & Smart Organizer)",
  "main": "dist/code.js",
  "scripts": {
    "build": "vite build && esbuild src/code/code.ts --bundle --outfile=dist/code.js --target=es2015",
    "build:watch": "concurrently \"vite build --watch\" \"esbuild src/code/code.ts --bundle --outfile=dist/code.js --watch\"",
    "lint": "eslint --ext .ts,.tsx --ignore-pattern node_modules ."
  },
  "dependencies": {
    "@ant-design/icons": "^5.2.6",
    "antd": "^6.2.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "simplebar": "^6.3.3",
    "simplebar-react": "^3.3.2"
  },
  "devDependencies": {
    "@figma/plugin-typings": "*",
    "@types/node": "^20.10.0",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@vitejs/plugin-react": "^4.2.1",
    "concurrently": "^8.2.2",
    "esbuild": "^0.19.0",
    "typescript": "^5.3.2",
    "vite": "^5.0.0",
    "vite-plugin-singlefile": "^2.0.1"
  }
}